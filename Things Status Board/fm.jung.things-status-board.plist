<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>fm.jung.things-status-board</string>
    <key>EnableGlobbing</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>osascript</string>
        <string>/Users/ELLIOTTCABLE/Library/System/Things Status Board/exportThingsTasks.scpt</string>
        <string>Today</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/ELLIOTTCABLE/Dropbox/Public/Things Status Board/tasks.csv</string>
    <key>StartInterval</key>
    <integer>600</integer>
</dict>
</plist>
