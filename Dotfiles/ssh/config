IdentityFile ~/.identity.insecure
HashKnownHosts yes
VisualHostKey yes

Compression yes
CompressionLevel 8

EscapeChar none

ControlMaster auto
ControlPath ~/.ssh/sockets/%r@%h:%p.sock
ControlPersist 30

Host ell.io
   ForwardAgent yes
   IdentityFile /Volumes/Key/.identity
   Ciphers arcfour256,arcfour128,blowfish-cbc,aes256-ctr
